<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="testmodule_form_view">
			<field name="name">testmodule.form</field>
			<field name="model">testmodule</field>
			<field name="arch" type="xml">
				<form string="testmodule" version="7.0">
					<sheet>
						<group>
							<field name="name" />
							<field name="input_a" on_change="on_change_number(input_a,input_b,context)" />
							<field name="input_b" on_change="on_change_number(input_a,input_b,context)" />
							<field name="result" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
	<record model="ir.actions.act_window" id="action_testmodule_form">
		<field name="name">testmodule</field>
		<field name="res_model">testmodule</field>
		<field name="context">{'testmodule':'Testyifgnglocal'}</field>
		<newline/>
	</record>
	
	<menuitem name="Test" icon="terp-project" id="one_id" />
	<menuitem name="Test" parent="one_id" id="two_id" />
	<menuitem name="Test Module" parent="two_id" id="three_id"
		action="action_testmodule_form" />	
	</data>
</openerp>